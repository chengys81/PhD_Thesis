	file = {ScienceDirect Full Text PDF:/Users/yongsheng/Library/Application Support/Firefox/Profiles/lv60gqk8.default/zotero/storage/G9HID99P/Stumvoll et al. - 2005 - Type 2 diabetes principles of pathogenesis and th.pdf:application/pdf;ScienceDirect Snapshot:/Users/yongsheng/Library/Application Support/Firefox/Profiles/lv60gqk8.default/zotero/storage/I9R84MZE/S014067360561032X.html:text/html}
}

