@article{kompoti_elevated_2006,
	title = {Elevated serum triglycerides is the strongest single indicator for the presence of metabolic syndrome in patients with type 2 diabetes},
	volume = {5},
	issn = {1475-2840},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1618385/},
	doi = {10.1186/1475-2840-5-21},
	abstract = {Background
Patients with diabetes already fulfill one diagnostic criterion for {MS} according to the existing classifications. Our aim was to identify one single clinical parameter, which could effectively predict the presence of {MS} in patients with type 2 diabetes.

Methods
We studied all patients with type 2 diabetes who attended our Diabetes Outpatient Clinic during a three-month period. Waist circumference, blood pressure and serum lipids were measured. Establishment of {MS} diagnosis was based a) on National Cholesterol Education Program Adult Treatment Panel {III} ({NCEP} {ATP} {III}) criteria and b) on International Diabetes Federation ({IDF}) criteria. Receiver operating characteristic ({ROC}) analysis was applied in order to identify the clinical parameter with the highest predictive capability for {MS}. Among the 500 participating patients (231 males, 269 females), {MS} was diagnosed in 364 patients (72.8\%) according to the {NCEP} {ATP} {III} criteria and in 408 patients (81.6\%) according to the {IDF} criteria.

Results
For the {NCEP} {ATP} {III} classification, serum triglycerides (in the overall population), waist and {HDL} (in female population) demonstrated the highest predictive capability for {MS} ({AUCs}:0.786, 0.805 and 0.801, respectively). For the {IDF} classification, no single parameter reached an {AUC} {\textgreater} 0.800 in the overall population. In females, {HDL} displayed a satisfactory predictive capability for {MS} with an {AUC} which was significantly higher than the one in males (0.785 vs. 0.676, respectively, p {\textless} 0.05).

Conclusion
Elevated serum triglycerides strongly indicate the presence of {MS} in patients with type 2 diabetes. In female patients with type 2 diabetes, central obesity was the second stronger predictor of {MS} besides hypertriglyceridemia.},
	pages = {21},
	journaltitle = {Cardiovascular Diabetology},
	shortjournal = {Cardiovasc Diabetol},
	author = {Kompoti, Maria and Mariolis, Anargiros and Alevizos, Alevizos and Kyrazis, Ioannis and Protopsaltis, Ioannis and Dimou, Eleni and Lentzas, Ioannis and Levisianou, Dimitrios and Gova, Afroditi and Melidonis, Andreas},
	urldate = {2014-11-25},
	date = {2006-10-04},
	pmid = {17020601},
	pmcid = {PMC1618385},
	file = {PubMed Central Full Text PDF:/Users/yongsheng/Library/Application Support/Firefox/Profiles/lv60gqk8.default/zotero/storage/HMZECCH8/Kompoti et al. - 2006 - Elevated serum triglycerides is the strongest sing.pdf:application/pdf}
}

