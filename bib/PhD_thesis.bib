	shortjournal = {Journal of Hepatology},
	author = {Bechmann, Lars P. and Hannivoort, Rebekka A. and Gerken, Guido and Hotamisligil, Gökhan S. and Trauner, Michael and Canbay, Ali},
	urldate = {2014-11-19},
	date = {2012-04},
	keywords = {{ER} stress, Fatty acid transporters, {HCC}, Liver regeneration, {NAFLD}, Nuclear receptors},
	file = {ScienceDirect Full Text PDF:/Users/yongsheng/Library/Application Support/Firefox/Profiles/lv60gqk8.default/zotero/storage/7ANNG83B/Bechmann et al. - 2012 - The interaction of hepatic lipid and glucose metab.pdf:application/pdf;ScienceDirect Snapshot:/Users/yongsheng/Library/Application Support/Firefox/Profiles/lv60gqk8.default/zotero/storage/XJ72RXFV/S016882781100852X.html:text/html}
