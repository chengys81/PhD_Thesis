	number = {16},
	journaltitle = {Journal of Biological Chemistry},
	author = {Wu, Jiansheng and Katrekar, Anjali and Honigberg, Lee A. and Smith, Ashley M. and Conn, Marion T. and Tang, Jie and Jeffery, Doug and Mortara, Kyle and Sampang, Jun and Williams, Steve R. and Buggy, Joseph and Clark, James M.},
	urldate = {2011-09-05},
	date = {2006-04-21},
	file = {Full Text PDF:/Users/yongsheng/Library/Application Support/Firefox/Profiles/lv60gqk8.default/zotero/storage/VB2ZSG56/Wu et al. - 2006 - Identification of Substrates of Human Protein-tyro.pdf:application/pdf}
}

@article{ogris_catalytically_1999,
