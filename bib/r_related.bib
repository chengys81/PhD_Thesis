	file = {PubMed Central Full Text PDF:/Users/yongsheng/Library/Application Support/Firefox/Profiles/lv60gqk8.default/zotero/storage/P3MM9N9K/Griswold-Prenner et al. - 1998 - Physical and Functional Interactions between Type .pdf:application/pdf}
